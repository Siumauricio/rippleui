{
  "name": "rippleui",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "types": "build/server.d.ts",
  "scripts": {
    "start": "npx tailwindcss -i ./css/globals.css -o ./dist/output.css --watch",
    "build:css": "tailwindcss --postcss -i ./components/styles.css -o ./dist/css/styles.css --watch",
    "build:minified": "tailwindcss --postcss -i ./components/styles.css -o ./dist/css/styles.min.css --watch",
    "build:production": "tailwindcss -i ./dist/css/styles.css -o ./dist/css/styles.production.css --minify ",
    "build:js": "tsc -b --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siumauricio/rippleui.git"
  },
