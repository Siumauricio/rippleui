  "bugs": {
    "url": "https://github.com/Siumauricio/rippleui/issues"
  },
  "homepage": "https://github.com/Siumauricio/rippleui#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "live-server": "^1.2.2",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "ts-node": "^10.9.1"
  }
