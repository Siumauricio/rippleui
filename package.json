{
  "name": "rippleui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "start": "start npm run build:css && start npm run build:js",
    "build:css": "tailwindcss --postcss -i ./components/styles.css -o ./dist/css/styles.css --watch",
    "build:minified": "tailwindcss --postcss -i ./components/styles.css -o ./dist/css/styles.min.css --watch",
    "build:production": "tailwindcss -i ./dist/css/styles.css -o ./dist/css/styles.production.css --minify ",
    "build:js": "tsc -b --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siumauricio/rippleui.git"
  },
  "keywords": [
    "ripple",
    "ui",
    "tailwindcss",
    "components",
    "modern",
    "clean",
    "css",
    "framework",
    "ui-library"
  ],
  "author": "Mauricio Siu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Siumauricio/rippleui/issues"
  },
  "homepage": "https://github.com/Siumauricio/rippleui#readme",
  "devDependencies": {
    "@types/postcss-js": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "postcss-js": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  }
}
